<Addin id="SQLiteChatLogger" version="1.0" isroot="false">
    <Runtime>
        <Import assembly="SQLiteChatLogger.dll" />
    </Runtime>
    <Dependencies>
        <Addin id="MoistureBot" version="1.0" />
    </Dependencies>
    <Extension path="/MoistureBot/IReceiveFriendChatMessages">
        <Type type="MoistureBot.SQLiteChatLogger" />
    </Extension>
    <Extension path="/MoistureBot/IReceiveGroupChatMessages">
        <Type type="MoistureBot.SQLiteChatLogger" />
    </Extension>
</Addin>